<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ClangPlugin" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="Clang" prefix_auto="0" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--no-ipc" />
				<Option host_application="/usr/bin/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="`pkg-config --cflags codeblocks`" />
					<Add option="`wx-config --cflags --version=2.8 --toolkit=gtk2" />
					<Add option="`" />
					<Add option="-fPIC" />
				</Compiler>
				<Linker>
					<Add option="`pkg-config --libs codeblocks`" />
					<Add option="`wx-config --libs --version=2.8 --toolkit=gtk2" />
					<Add option="`" />
					<Add library="clang" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 Clang.zip manifest.xml" />
					<Add after="zip -j9 Clang.cbplugin Clang.so Clang.zip" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
		</Compiler>
		<Unit filename="Clang.cpp" />
		<Unit filename="Clang.h" />
		<Unit filename="ClangThread.cpp" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<lib_finder disable_auto="1" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
